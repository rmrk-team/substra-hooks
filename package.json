{
  "name": "substra-hooks",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "wsrun -e -c -s -x example-nextjs build",
    "test": "wsrun -e -c -s --exclude-missing test",
    "release": "yarn build && yarn changeset publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@changesets/cli": "^2.17.0",
    "prettier": "2.4.1",
    "wsrun": "^5.2.1"
  }
}
